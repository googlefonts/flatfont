namespace FlatFont;

// https://unifiedfontobject.org/versions/ufo3/metainfo.plist/
table MetaInfo {
  creator: string;
  format_version: ushort = 3;
  format_version_minor: uint = 0;
}

// http://unifiedfontobject.org/versions/ufo3/fontinfo.plist/
table FontInfo {
  meh: string;
}

table FlatFont {
  meta: MetaInfo;
  font_info: FontInfo;
}

root_type FlatFont;